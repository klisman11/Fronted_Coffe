{"ast":null,"code":"import SwiperCore, { Pagination } from 'swiper/core';\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport axios from 'axios';\nSwiperCore.use([Pagination]);\nexport default {\n  name: 'Testimonial',\n  components: {\n    Swiper: Swiper,\n    SwiperSlide: SwiperSlide\n  },\n  data: function data() {\n    return {\n      categorias: [],\n      swiperOptions: {\n        pagination: {\n          el: '.testimonial-area .pagination-dot-style',\n          type: 'bullets',\n          clickable: true\n        }\n      },\n      bgImg: {\n        backgroundImage: \"url(\".concat(require('../assets/img/clients-bg.jpg'), \")\")\n      },\n      clients: [{\n        clientThumb: require(\"../assets/img/testimonial/1.png\"),\n        name: \"MOHAMET LUIS\",\n        designation: \"Web Designer\",\n        desc: \"Claritas est etiam processus dynamicus, qui sequitur mutationem consuetudium lectorum. Mirum est notare quam littera gothica, quam nunc putamus parum claram, anteposuerit litterarum formas humanitatis per seacula quarta decima et quinta decima.\"\n      }, {\n        clientThumb: require(\"../assets/img/testimonial/2.png\"),\n        name: \"ERIN MAYA\",\n        designation: \"Web Developer\",\n        desc: \"Claritas est etiam processus dynamicus, qui sequitur mutationem consuetudium lectorum. Mirum est notare quam littera gothica, quam nunc putamus parum claram, anteposuerit litterarum formas humanitatis per seacula quarta decima et quinta decima.\"\n      }, {\n        clientThumb: require(\"../assets/img/testimonial/3.png\"),\n        name: \"KUNJO PANDE\",\n        designation: \"Marketer\",\n        desc: \"Claritas est etiam processus dynamicus, qui sequitur mutationem consuetudium lectorum. Mirum est notare quam littera gothica, quam nunc putamus parum claram, anteposuerit litterarum formas humanitatis per seacula quarta decima et quinta decima.\"\n      }]\n    };\n  },\n  mounted: function mounted() {\n    this.getproducto();\n  },\n  methods: {\n    getproducto: function getproducto() {\n      var _this = this;\n\n      axios.get('http://localhost:8081/api/productos').then(function (res) {\n        var cat = res.data.productos;\n        _this.categorias = [cat];\n        console.log('datos', _this.categorias);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAwCI,OAAOA,UAAP,IAAqBC,UAArB,QAAuC,aAAvC;AACA,SAASC,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACAJ,UAAU,CAACK,GAAX,CAAe,CAACJ,UAAD,CAAf;AAEA,eAAe;EACXK,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IACRL,MAAM,EAANA,MADQ;IAERC,WAAU,EAAVA;EAFQ,CAFD;EAOXK,IAPW,kBAOJ;IACH,OAAO;MACHC,UAAU,EAAC,EADR;MAKHC,aAAa,EAAE;QACXC,UAAU,EAAE;UACRC,EAAE,EAAE,yCADI;UAERC,IAAI,EAAE,SAFE;UAGRC,SAAS,EAAE;QAHH;MADD,CALZ;MAaHC,KAAK,EAAE;QACHC,eAAe,gBAASC,OAAO,CAAC,8BAAD,CAAhB;MADZ,CAbJ;MAgBHC,OAAO,EAAE,CACL;QACIC,WAAW,EAAEF,OAAO,CAAC,iCAAD,CADxB;QAEIX,IAAI,EAAE,cAFV;QAGIc,WAAW,EAAE,cAHjB;QAIIC,IAAI,EAAE;MAJV,CADK,EAOL;QACIF,WAAW,EAAEF,OAAO,CAAC,iCAAD,CADxB;QAEIX,IAAI,EAAE,WAFV;QAGIc,WAAW,EAAE,eAHjB;QAIIC,IAAI,EAAE;MAJV,CAPK,EAaL;QACIF,WAAW,EAAEF,OAAO,CAAC,iCAAD,CADxB;QAEIX,IAAI,EAAE,aAFV;QAGIc,WAAW,EAAE,UAHjB;QAIIC,IAAI,EAAE;MAJV,CAbK;IAhBN,CAAP;EAqCH,CA7CU;EA8CXC,OA9CW,qBA8CF;IACL,KAAKC,WAAL;EACH,CAhDU;EAkDXC,OAAO,EAAC;IACJD,WADI,yBACS;MAAA;;MACTnB,KAAK,CAACqB,GAAN,CAAU,qCAAV,EACCC,IADD,CACM,aAAG,EAAE;QAEH,IAAMC,GAAG,GAAEC,GAAG,CAACpB,IAAJ,CAASqB,SAApB;QACA,KAAI,CAACpB,UAAL,GAAiB,CAACkB,GAAD,CAAjB;QACAG,OAAO,CAACC,GAAR,CAAY,OAAZ,EAAoB,KAAI,CAACtB,UAAzB;MAEP,CAPD;IAQJ;EAVI;AAlDG,CAAf","names":["SwiperCore","Pagination","Swiper","SwiperSlide","axios","use","name","components","data","categorias","swiperOptions","pagination","el","type","clickable","bgImg","backgroundImage","require","clients","clientThumb","designation","desc","mounted","getproducto","methods","get","then","cat","res","productos","console","log"],"sourceRoot":"","sources":["/home/klisman/Documentos/Project_Node_Vue/Coffe_vue/Fronted_Coffe/Energi/src/components/Testimonial.vue"],"sour